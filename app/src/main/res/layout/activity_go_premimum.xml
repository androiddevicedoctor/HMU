            android:layout_marginStart="0dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="10dp"
            android:drawableLeft="@drawable/check_24"
            android:drawablePadding="10dp"
            android:fontFamily="@font/modern_era_egular"
